CREATE TABLE `admin`(    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,    `username` VARCHAR(255) NOT NULL,    `password` VARCHAR(255) NOT NULL,    `role` VARCHAR(255) NOT NULL,    `first_name` VARCHAR(255) NOT NULL,    `last_name` VARCHAR(255) NOT NULL,    `middle_name` VARCHAR(255) NOT NULL);CREATE TABLE `student`(    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,    `first_name` VARCHAR(255) NULL,    `last_name` VARCHAR(255) NOT NULL,    `middle_name` VARCHAR(255) NOT NULL,    `certificate` VARCHAR(255) NOT NULL,    `course` VARCHAR(255) NOT NULL,    `requirements` VARCHAR(255) NOT NULL,    `address` VARCHAR(255) NOT NULL,    `gender` VARCHAR(255) NOT NULL,    `birth_date` VARCHAR(255) NOT NULL,    `contact_number` VARCHAR(255) NOT NULL,    `username_student_number` VARCHAR(255) NOT NULL,    `password` VARCHAR(255) NOT NULL);CREATE TABLE `requirements`(    `id` INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,    `requirements_type` VARCHAR(255) NOT NULL,    `requirements_name` VARCHAR(255) NOT NULL,    `student_id` INT NOT NULL);CREATE TABLE `table_4`(    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY);CREATE TABLE `course`(    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,    `course_name` VARCHAR(255) NOT NULL,    `category` VARCHAR(255) NOT NULL,    `duration` VARCHAR(255) NOT NULL,    `slot` INT NOT NULL,    `training_center_id` INT NOT NULL,    `status` VARCHAR(255) NOT NULL DEFAULT '' Available '');CREATE TABLE `training_center`(    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,    `Training_center_name` VARCHAR(255) NOT NULL,    `location` VARCHAR(255) NOT NULL,    `course` VARCHAR(255) NOT NULL,    `requirements_id` VARCHAR(255) NOT NULL,    `email` VARCHAR(255) NOT NULL,    `phone` VARCHAR(255) NOT NULL,    `facebook` VARCHAR(255) NOT NULL,    `website` VARCHAR(255) NOT NULL,    `map_link` VARCHAR(255) NOT NULL,    `description` TEXT NOT NULL);CREATE TABLE `category`(    `id` INT NOT NULL,    `category_name` VARCHAR(255) NOT NULL,    `icons_image` VARCHAR(255) NOT NULL,    PRIMARY KEY(`category_name`));ALTER TABLE    `training_center` ADD CONSTRAINT `training_center_requirements_id_foreign` FOREIGN KEY(`requirements_id`) REFERENCES `requirements`(`id`);ALTER TABLE    `requirements` ADD CONSTRAINT `requirements_student_id_foreign` FOREIGN KEY(`student_id`) REFERENCES `student`(`id`);ALTER TABLE    `course` ADD CONSTRAINT `course_training_center_id_foreign` FOREIGN KEY(`training_center_id`) REFERENCES `training_center`(`id`);